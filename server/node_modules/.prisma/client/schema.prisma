generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model SpinResult {
  id        Int      @id @default(autoincrement())
  segment   Int // Assuming you already changed to Int as per previous steps
  userName  String
  userEmail String?  @unique // Now nullable with ?
  timestamp DateTime @default(now())
}
